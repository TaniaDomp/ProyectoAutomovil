<Window x:Class="ProyectoAutomovil.Baja"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoAutomovil"
        mc:Ignorable="d"
        Title="Baja" Height="286" Width="482" Icon="coche-ecologico.png">
    <Grid Background="#FFBBC0F4">
        <Label Content="IdAuto" HorizontalAlignment="Left" Margin="64,70,0,0" VerticalAlignment="Top" FontSize="25" FontFamily="Segoe UI Light"/>
        <Button x:Name="btBaja" Content="Baja" HorizontalAlignment="Left" Margin="64,164,0,0" VerticalAlignment="Top" FontSize="30" Click="btBaja_Click" Width="150" Height="50" FontFamily="Segoe UI Light" Background="#FFCBF4F6"/>
        <Button x:Name="btRegresar" Content="Regresar" HorizontalAlignment="Left" Margin="270,164,0,0" VerticalAlignment="Top" FontSize="30" Click="btRegresar_Click" Height="50" Width="150" FontFamily="Segoe UI Light" Grid.ColumnSpan="2" Background="#FFCBF4F6"/>
        <TextBox x:Name="tbFolio" HorizontalAlignment="Left" Margin="188,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240" FontSize="25" FontFamily="Segoe UI Light" Height="49" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
